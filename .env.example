# ==============================================
# API KEYS - External Services
# ==============================================

# Google Gemini API Key
# Obtenha em: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Tavily API Key (Web Search)
# Obtenha em: https://tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here

# ==============================================
# DATABASE - PostgreSQL Configuration
# ==============================================

# PostgreSQL User
POSTGRES_USER=SGC_USER

# PostgreSQL Password
POSTGRES_PASSWORD=SGC_DB123

# PostgreSQL Database Name
POSTGRES_DB=SGC_DB

# PostgreSQL Host (use 'database' for Docker, 'localhost' for local development)
POSTGRES_HOST=database

# PostgreSQL Port (use 5432 for Docker internal, 5436 for local development)
POSTGRES_PORT=5432

